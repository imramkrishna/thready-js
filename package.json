{
  "name": "thready-js",
  "version": "1.0.1",
  "description": "A lightweight, type-safe thread pool manager for JavaScript and TypeScript. Bring your own worker - we handle the pooling.",
  "keywords": [
    "worker",
    "threads",
    "multithreading",
    "thread-pool",
    "web-worker",
    "worker-pool",
    "parallel",
    "concurrent",
    "async",
    "typescript",
    "performance"
  ],
  "homepage": "https://github.com/imramkrishna/Thready#readme",
  "bugs": {
    "url": "https://github.com/imramkrishna/Thready/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imramkrishna/Thready.git"
  },
  "license": "ISC",
  "author": "Ram Krishna Yadav",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup --config tsup.config.ts",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  },
  "bin": {
    "thready": "./dist/cli.js"
  }
}
